<Window x:Class="Interactipy.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Interactipy"
        mc:Ignorable="d"
        Title="Interactipy" Height="343.507" Width="531.629" Loaded="Window_Loaded" Closed="Window_Closed">

    
    
    <Grid>
        <TextBox x:Name="txt_output" Margin="10,10,9.6,32.6" PreviewKeyDown="txt_output_PreviewKeyDown" AcceptsTab="True"/>
        <TextBox x:Name="txt_input" VerticalAlignment="Bottom" Margin="10,0,9.6,9.6" KeyDown="txt_input_KeyDown">
            <TextBox.InputBindings>
                <KeyBinding Modifiers="Ctrl" Key="C" Command="{Binding Command}"/>
            </TextBox.InputBindings>
        </TextBox>
    </Grid>
</Window>
